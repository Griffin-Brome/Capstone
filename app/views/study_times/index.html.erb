<p id="notice"><%= notice %></p>

<h1>Study Times</h1>

<table>
  <thead>
    <tr>
      <th>Start</th>
      <th>End</th>
      <th>Deadline</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_times.each do |study_time| %>
      <tr>
        <td><%= study_time.start %></td>
        <td><%= study_time.end %></td>
        <td><%= study_time.deadline_id %></td>
        <td><%= link_to 'Show', study_time %></td>
        <td><%= link_to 'Edit', edit_study_time_path(study_time) %></td>
        <td><%= link_to 'Destroy', study_time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study Time', new_study_time_path %>
