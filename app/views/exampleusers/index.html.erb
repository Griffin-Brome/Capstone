<p id="notice"><%= notice %></p>

<h1>Exampleusers</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exampleusers.each do |exampleuser| %>
      <tr>
        <td><%= exampleuser.email %></td>
        <td><%= exampleuser.password %></td>
        <td><%= link_to 'Show', exampleuser %></td>
        <td><%= link_to 'Edit', edit_exampleuser_path(exampleuser) %></td>
        <td><%= link_to 'Destroy', exampleuser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exampleuser', new_exampleuser_path %>
